<jboss:ejb-jar xmlns:jboss="http://www.jboss.com/xml/ns/javaee"
               xmlns:jee="http://java.sun.com/xml/ns/javaee"
               xmlns:ci="urn:container-interceptors:1.0"
               xmlns="http://www.jboss.com/xml/ns/javaee"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee http://www.jboss.org/j2ee/schema/jboss-ejb3-2_0.xsd"
               version="3.1"
               impl-version="2.0">

    <!-- this can add an additional distinct identifier for the EJB application.
         It is needed to add this to the EJB lookup string.
    <distinct-name>Wolf</distinct-name>
    -->

    <!-- Class level container-interceptor which will be applicable for all business method
         invocations on the bean
    <jee:assembly-descriptor>
        <ci:container-interceptors>
            <jee:interceptor-binding>
                <ejb-name>*</ejb-name>
                <interceptor-class>org.jboss.wfink.ejb31.interceptor.SimpleInterceptor</interceptor-class>
            </jee:interceptor-binding>
        </ci:container-interceptors>
    </jee:assembly-descriptor>
    -->

</jboss:ejb-jar>

